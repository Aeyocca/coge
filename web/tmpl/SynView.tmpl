function changeColor(id, color) {
  document.getElementById(id).style.color = color;
}

var txtboxes = ['dr1up','dr1down','dr2up','dr2down','dr3up','dr3down'];

function setup(){
   for(var k in txtboxes){
       k = txtboxes[k];
       var el = get(k);
       el.onkeyup = function(){
           var v = this.value;
	   if(get('apply_all').checked){
		   for(var k in txtboxes){
		       k = txtboxes[k];
		       get(k).value = v;
		   }
	   }
	  }
       }
  if (kaj.GS('accn1')) {dataset_search(['accn1','args__1'],[feat_search_chain]);}
  if (kaj.GS('accn2')) {dataset_search(['accn2','args__2'],[feat_search_chain]);}
  if (kaj.GS('accn3')) {dataset_search(['accn3','args__3'],[feat_search_chain]);}
}


function set_hsp_colors (type)
 {
  if (type == "rasta")
   {
    kaj.GS('hsp1r',255)
    kaj.GS('hsp1g',0)
    kaj.GS('hsp1b',0)
    kaj.GS('hsp2r',255)
    kaj.GS('hsp2g',255)
    kaj.GS('hsp2b',0)
    kaj.GS('hsp3r',51)
    kaj.GS('hsp3g',102)
    kaj.GS('hsp3b',51)
   }
  else if (type == "cblind")
   {
    kaj.GS('hsp1r',110)
    kaj.GS('hsp1g',110)
    kaj.GS('hsp1b',110)
    kaj.GS('hsp2r',200)
    kaj.GS('hsp2g',200)
    kaj.GS('hsp2b',150)
    kaj.GS('hsp3r',220)
    kaj.GS('hsp3g',220)
    kaj.GS('hsp3b',220)
   }
  else if (type == "fire")
   {
    kaj.GS('hsp1r',255)
    kaj.GS('hsp1g',0)
    kaj.GS('hsp1b',0)
    kaj.GS('hsp2r',255)
    kaj.GS('hsp2g',125)
    kaj.GS('hsp2b',0)
    kaj.GS('hsp3r',255)
    kaj.GS('hsp3g',255)
    kaj.GS('hsp3b',0)
   }
  else 
   {
    kaj.GS('hsp1r',150)
    kaj.GS('hsp1g',150)
    kaj.GS('hsp1b',255)
    kaj.GS('hsp2r',100)
    kaj.GS('hsp2g',255)
    kaj.GS('hsp2b',100)
    kaj.GS('hsp3r',255)
    kaj.GS('hsp3g',100)
    kaj.GS('hsp3b',100)
   }
  
 }

function update_distance (check, val) {
  return;
  check = get(check);
  val = get(val);
//  alert (check, val);
  if (check.value == 0) { return; }
  else {
    alert(val);
  }
}

function get(id){
    return document.getElementById(id);
}

function feat_search_chain(val, num) {
 if (val) {
  kaj.GS('source'+num, val);
  feat_search(['accn'+num,'dsid'+num, 'args__'+num],['feat'+num]);
 }
// else {
 //  alert(num);
 //  kaj.GS('source'+num,'<input type="hidden" id="dsid'+num+'">');
 //  kaj.GS('feat'+num,'<input type="hidden" id="featid'+num+'">');
// }
}

//this func switching the "left" and "right" sequence values when reverse region display is selected
function rev_locs (val) {
  up = kaj.GS('dr'+val+'up');
  down = kaj.GS('dr'+val+'down');
  kaj.GS('dr'+val+'up', down);
  kaj.GS('dr'+val+'down', up);
}

