<SCRIPT language="JavaScript">

function get_seq(url,radio) {
 if (url == 1)
  {alert('You have not selected a BLAST program! Please select a program to run.');}
 else {
 seq = $('#seq_box').val();
 word_size = $('#word_size').val();
 expect = $('#e_value').val();
 exist = $('#gap_exist').val();
 extent = $('#gap_ext').val();
 gapcost = exist+' '+extent;
 job_title = $('#job_title').val();
 url = url+'&QUERY='+seq+'&JOB_TITLE='+job_title+'&WORD_SIZE='+word_size;
 //+'&DATABASE='+db;
 //alert(''+radio);
 if (radio == 'blast_type_n') {
 nuc_reward = $('#match_score').val();
 nuc_penalty = $('mismatch_score').val();
 match_score = nuc_reward+','+nuc_penalty;
 //alert('I ran!');
 window.open(url+'&MATCH_SCORES='+match_score);
 }
//alert(''+url);
else {
 window.open(url);}
 }
}

