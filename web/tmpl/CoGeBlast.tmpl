function radio_lock(unlock) {
  var1 = document.getElementById('select_n');
  var2 = document.getElementById('select_p');
  if (unlock == 2) {
  var1.style.display = "none";
  var2.style.display = "block";
  }
  else {
  var1.style.display = "block";
  var2.style.display = "none";
  }
}

function select_blast(radio) {
  get_url([radio,'e_value','blast_db','bn'],[get_seq]);
}

function changeColor(id, color) {
  document.getElementById(id).style.color = color;
}

function check_seq_for_blast() {
 accn = $('#accn_select').val() || 0;
 dsid = $('#dsid').val() || 0;
 featid = $('#featid').val() || 0;
 chr = $('#chr').val() || 0;
// radio2 = kaj.GS('bn');
// alert('bn');
 type_name = $('#Type_name').val() || $('#featid').val() || 0; 
 if ((accn && dsid && type_name) || (chr && dsid)) {
  if (featid) {
   get_sequence(['accn_select','featid','dsid','bn', 'seqview', 'upstream','downstream','rc'],['seq_box']);
  }
  else if (chr) {
   get_sequence(['accn_select','chr','dsid','bn', 'seqview', 'upstream','downstream'],['seq_box']);
   }
  else {
// alert('Im being called!'+accn+dsid+type_name);
 get_sequence(['accn_select','Type_name','dsid','bn'],['seq_box']);}
}
}
 
function search_chain(val) {
 minlen = 4;
 if ((val == 1) && ($('#accn').val().length > minlen)){
   return;
 } 
 $('#DS_title').val('');
 $('#FT_title').val('');
 $('#accn_list').val('<input type="hidden" id="accn_select">');	
 $('#Source').val('<input type="hidden" id="dsid">');	
 $('#FeatType').val('<input type="hidden" id="Type_name">');	
 if (val == 1 || $('#accn').val().length > minlen) {
//   kaj.GS('DS_title','<font class="loading">Searching. . .</font>');// for "'+kaj.GS('accn')+'" type '+''+'. . .</font>');
   cogesearch(['accn','type', 'org'],[source_search_chain]); 
 }
}

function source_search_chain (val1) {
 $('#DS_title').val('');
 $('#FT_title').val('');
 $('#Source').val('<input type="hidden" id="dsid"><font class="loading">Loading. . .</font>');	
 $('#FeatType').val('<input type="hidden" id="Type_name">');		
 if (val1) {$('#accn_list').val(val1);}
 source_search(['accn_select'], [get_types_chain]);
}

function get_types_chain (val1, val2) {
 $('#FT_title').val('');
 $('#FeatType').val('<input type="hidden" id="Type_name"><font class="loading">Loading. . .</font>');	
 if (val1) {$('#Source').val(val1);}
// if (val2) {kaj.GS('DS_title', "Dataset");}
 get_types(['accn_select', 'dsid'],[get_anno_chain]);	
}

function get_anno_chain (val1, val2) {
 if (val1) {$('#FeatType').val(val1)};
// if (val2) {kaj.GS('FT_title', "Type Name");}
 as = $('#accn_select').val();
// v = kaj.G('version');
 tn = $('#Type_name').val();
 $('#anno_title').val("Feature Annotations");
 get_sequence(['accn_select', 'Type_name', 'dsid'],['seq_box']);
 get_pretty_print(['accn_select','Type_name', 'dsid'],['pretty_print_anno']);
}

function blast_params() {
 blast_param(['bn'],['pro_or_nu_param']);
}

function blast_param_on_select(radio) {
 program = $('#'+radio).val();
 //alert(''+program);
 if ((program == 'blastx') || (program == 'tblastx')) {
 //alert('Program is true!');
  $('#program_type').val("blast_type_p");
  blast_param(['program_type','program_type'],['pro_or_nu_param']);
  }
  else {
   blast_param([],['pro_or_nu_param']);}
}

function displayFeature() {
if(document.check_feat.feature_display.checked == true) {
// alert('program being called');
get_pretty_print(['accn_select','featid', 'dsid'],['pretty_print_anno']);
}
else {
 //alert('else being called');
 $('#pretty_print_anno').html('');}
}

function fill_param_on_start() {
  blast_param([],['pro_or_nu_param'])
};

fill_param_on_start();

</SCRIPT>
