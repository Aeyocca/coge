function accn_search_chain(val) {
 minlen = 4;
 GS('DS_title','');
 GS('FT_title','');
 if ((val == 1) && (GS('accn').length > minlen)){
   return;
 } else if (val == 1) {
   accn_search(['accn', "args__"+val],[source_search_chain]); 
 } else if  (GS('accn').length > minlen) {
   accn_search(['accn', "args__"+val],[source_search_chain]); 
 }

}

function source_search_chain (val) {
 if (val) {GS('accn_list',val);}
 source_search(['accn_select'], [get_types_chain]);
}

function get_types_chain (val1, val2) {
 if (val1) {GS('Source', val1);}
 if (val2) {GS('DS_title', "Data Version");}	
 get_types(['accn_select', 'infoid'],[get_anno_chain]);	
}

function get_anno_chain (val1, val2) {
 if (val1) {GS('FeatType', val1)};
 if (val2) {GS('FT_title', "Type Name");}
 get_anno(['accn_select', 'infoid', 'Type_name'],['anno']);	
}


</SCRIPT>
