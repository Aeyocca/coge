	function run_get_gc(opts){
	  if (!opts){opts={};}
	  dsgid = opts.dsgid;
	  divid1 = opts.divid1;
	  divid2 = opts.divid2;
	  divid3 = opts.divid3;
	  divid4 = opts.divid4;

	  // create a new closure who's scope does *not*
	  // include the `divid` value so that callback will 
	  // not clobber the `divid` variable 
	  var create_callback = function(divid1, divid2, divid3, divid4) {
	      var local_divid1 = divid1;
	      var local_divid2 = divid2;
	      var local_divid3 = divid3;
	      var local_divid4 = divid4;
	      return function (data) {
	          items = data.split('_');
	          $('#'+local_divid1).html(items[0]);
	          $('#'+local_divid2).html(items[1]);
	          $('#'+local_divid3).html(items[2]);
	          $('#'+local_divid4).html(items[3]);
	      };
	  };

	  $.ajax({
	    data: {
	 	    jquery_ajax: 1,
	       	    fname: 'get_gc',
	    	    dsgid: dsgid 
	          },
	    success : create_callback(divid1, divid2, divid3, divid4)
	  });
	}

