	//Filters Rows for search_text - Case sensitive as of now, and searches hidden columns...
	function filter_rows (search_text){
	  if(! search_text) { $("#genome_table tr:hidden").show(0);}
	  else if (search_text.length >= 3) {
	    $("#genome_table_body tr:hidden").show(0);
	    if( $('#search_type').val() == 1 ) { $("#genome_table_body tr:not(:contains('"+search_text+"'))").hide(); }
	    else { $("#genome_table_body tr:contains('"+search_text+"')").hide(); }
	  }
	  else return;
	}

	function feature_checkboxes(){
	 var action = $('#checked_action').val();
	 var accn=",";
	 $('#genome_table :checkbox').each(function(){
	    if (this.checked == true) accn = accn + this.id+",";
	  });
	 if (accn == ",") {
	   alert("Danger Will Robinson!\n1D-10T error!  You have not checked any genomes.");
	   return;
	 }
	 if (action == "fasta"){
	   $('#retrieve_message').html('Retrieving data');
	   $.ajax({
	    data: {
	 	   jquery_ajax: 1,
	           fname: 'get_fasta_seqs',
	    	   accn: accn
	          },
	    success : export_url
	   });
	 }
	 else if (action == "xls"){
	   $('#retrieve_message').html('Retrieving data');
	   
	   $.ajax({
	    data: {
	 	    jquery_ajax: 1,
	           fname: 'generate_excel_file',
	    	   accn: accn
	          },
	    success : export_url
	  });
	 }
	 else if (action == "csv"){
	   $('#retrieve_message').html('Retrieving data');
	   
	   $.ajax({
	    data: {
	 	    jquery_ajax: 1,
	           fname: 'generate_csv_file',
	    	   accn: accn
	          },
	    success : export_url
	  });
	 }
	 else if (action == "blast"){
	   $('#retrieve_message').html('Loading CoGeBlast');
	   $.ajax({
	    data: {
	 	   jquery_ajax: 1,
	           fname: 'send_to_CoGeBlast',
	    	   accn: accn
	          },
	    success : export_url
	  });
	 }
	 else if (action == "list"){
	   $('#retrieve_message').html('Loading GenomeList');
	   $.ajax({
	    data: {
	 	   jquery_ajax: 1,
	           fname: 'send_to_GenomeList',
	    	   accn: accn
	          },
	    success : export_url
	  });
	 }
	 else if (action == "synfind"){
	   $('#retrieve_message').html('Loading SynFind');
	   $.ajax({
	    data: {
	 	   jquery_ajax: 1,
	           fname: 'send_to_SynFind',
	    	   accn: accn
	          },
	    success : export_url
	  });
	 }
	 else if (action == "align"){
	   $('#retrieve_message').html('Loading GenomeAlign');
	   $.ajax({
	    data: {
	 	   jquery_ajax: 1,
	           fname: 'send_to_msa',
	    	   accn: accn
	          },
	    success : export_url
	  });
	 }
	 else {
	   alert("We will get this working ... soon.");
	 }
	}



	function export_url(url,num_accns) {
	 if (url == "alert") {
	   var remove = num_accns - 20;
	   alert("You have exceeded the number of features you can send to GEvo ( 20 Max ). You currently have "+num_accns+" selected. Please uncheck "+remove+" of your checked item(s).");
	 }
	 else {
	   var regex = /xls$/;
	   if ($('#gstid').val() && !regex.test(url))
	    {
	      url=url+';gstid='+$('#gstid').val();
	    }
	   window.open(url);
	 }
	 $('#retrieve_message').html(' ');

	}


