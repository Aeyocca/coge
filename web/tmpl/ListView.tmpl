function send_list_to() {
	var action = $('#checked_action').val();
	
	$.ajax({
		data: {
			jquery_ajax: 1,
			fname: action,
			lid: <TMPL_VAR NAME=LID>
		},
		success : function(val) {
			var items = jQuery.parseJSON(val);
			if (items.alert) {
				alert(items.alert);
			}
			if (items.url) {
				window.open(items.url, '_blank');
			}
		}
	});
}
