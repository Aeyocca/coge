<TMPL_IF NAME="MAIN">

<TMPL_INCLUDE NAME="widgets/JBrowse.tmpl">

<SCRIPT type="text/javascript">

$(function() {
	// Initialize dialog boxes
	$(".dialog_box").dialog({autoOpen: false, resizable: false});

	// Initialize globals
	timestamps = new Array();
	
	// Initialize AJAX
	$.ajaxSetup({
		type: "GET",
		url: "<TMPL_VAR NAME='PAGE_NAME'>",
		dataType: "html",
		cache: false,
	});
	
	// Initialize JBrowse widget
	init_JBrowse('browser_panel',
		function(browser) {
			jbrowse = browser;
		}
	);
});

</SCRIPT>

<div id="browser_panel" style="height:100%; width:100%; padding:0; border:0;"></div>

