                        <option value=".000001">1e-6</option>
                        <option value=".00001">1e-5</option>
                        <option value=".0001">0.0001</option>
                        <option value=".001" selected>0.001</option>
                        <option value=".01">0.01</option>
                        <option value=".1">0.1</option>
                        <option value="1">1</option>
                        <option value="10">10</option>
                        <option value="100">100</option>
                     </select>
                    </td>
                  </tr>
                  <tr>
                    <td>Word size:</td>
                    <td>
                      <input type='text' id='word_size' value="7" size="7" />
